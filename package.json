{
	"name": "functions",
	"version": "1.0.0",
	"description": "An example of setting up subscriptions with Apollo Server 4",
	"scripts": {
		"lint": "tslint --project tsconfig.json",
		"clear": "rm -rf ./lib",
		"build": "tsc",
		"serve": "npm run clear && npm run build",
		"shell": "npm run build && firebase functions:shell",
		"start": "ts-node src/index.ts",
		"clean-cache": "npm cache clean --force"
	},
	"engines": {
		"node": "18"
	},
	"main": "lib/index.js",
	"dependencies": {
		"@apollo/server": "^4.9.5",
		"@graphql-tools/schema": "^9.0.13",
		"axios": "^0.26.0",
		"cors": "^2.8.5",
		"dotenv": "^16.0.0",
		"express": "^4.17.1",
		"firebase-admin": "^11.11.0",
		"graphql": "^16.8.1",
		"graphql-redis-subscriptions": "^2.4.2",
		"graphql-resolvers": "^0.4.2",
		"graphql-subscriptions": "^2.0.0",
		"graphql-tag": "^2.12.6",
		"graphql-type-json": "^0.3.2",
		"graphql-ws": "^5.14.2",
		"jsonwebtoken": "^8.5.1",
		"jwks-rsa": "^2.0.5",
		"ts-node": "^10.5.0",
		"tslint": "^5.20.1",
		"typescript": "^4.7.4",
		"ws": "^8.14.2"
	},
	"devDependencies": {
		"@types/cors": "^2.8.13",
		"@types/node": "^18.6.3",
		"@types/ws": "^8.2.2"
	},
	"private": true
}
